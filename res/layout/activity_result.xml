<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/pic"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context=".Result" >

    <TextView
        android:id="@+id/textview1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="16dp"
        android:text="VIEW RESULT"
        android:textColor="#ffffff"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/textview3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/textview2"
        android:layout_below="@+id/textview2"
        android:layout_marginTop="24dp"
        android:text="Question2:"
        android:textColor="#ffffff"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/textview4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/textview3"
        android:layout_below="@+id/textview3"
        android:layout_marginTop="20dp"
        android:text="Question3:"
        android:textColor="#ffffff"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/textview5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/textview4"
        android:layout_below="@+id/textview4"
        android:layout_marginTop="22dp"
        android:text="Question4:"
        android:textColor="#ffffff"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/textview6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/textview5"
        android:layout_below="@+id/textview5"
        android:layout_marginTop="24dp"
        android:text="Question5:"
        android:textColor="#ffffff"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/textview7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/textview6"
        android:layout_below="@+id/textview6"
        android:layout_marginTop="22dp"
        android:text="Question6:"
        android:textColor="#ffffff"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/textview2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textview1"
        android:layout_marginRight="20dp"
        android:layout_marginTop="14dp"
        android:layout_toLeftOf="@+id/textview1"
        android:text="Question1:"
        android:textColor="#ffffff"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/tres7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/textview8"
        android:layout_toRightOf="@+id/textView9"
        android:text="Medium text"
        android:textColor="#ffffff" />

    <TextView
        android:id="@+id/tres1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/textview3"
        android:layout_toRightOf="@+id/textView10"
        android:text="Medium text"
        android:textColor="#ffffff" />

    <TextView
        android:id="@+id/tres2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/textview4"
        android:layout_toRightOf="@+id/textView10"
        android:text="Medium text"
        android:textColor="#ffffff" />

    <TextView
        android:id="@+id/tres3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/textview4"
        android:layout_alignBottom="@+id/textview4"
        android:layout_alignLeft="@+id/tres7"
        android:text="Medium text"
        android:textColor="#ffffff" />

    <TextView
        android:id="@+id/tres4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/textview5"
        android:layout_alignBottom="@+id/textview5"
        android:layout_toRightOf="@+id/textView10"
        android:text="Medium text"
        android:textColor="#ffffff" />

    <TextView
        android:id="@+id/tres5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/textview7"
        android:layout_alignLeft="@+id/tres4"
        android:text="Medium text"
        android:textColor="#ffffff" />

    <TextView
        android:id="@+id/textview8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/textview7"
        android:layout_below="@+id/textview7"
        android:layout_marginTop="22dp"
        android:text="Question7:"
        android:textColor="#ffffff"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/tres6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/textview8"
        android:layout_alignBottom="@+id/textview8"
        android:layout_toRightOf="@+id/textView9"
        android:text="Medium text"
        android:textColor="#ffffff" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_below="@+id/textview8"
        android:layout_marginTop="31dp"
        android:text="no. of right answers"
        android:textColor="#ffffff"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/textView9"
        android:layout_below="@+id/textView9"
        android:layout_marginTop="14dp"
        android:text="no.of wrong answers"
        android:textColor="#ffffff"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/nowrong"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/textView10"
        android:layout_alignBottom="@+id/textView10"
        android:layout_alignRight="@+id/textview1"
        android:text="0"
        android:textColor="#ffffff" />

    <TextView
        android:id="@+id/noright"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/textView9"
        android:layout_alignBottom="@+id/textView9"
        android:layout_alignLeft="@+id/nowrong"
        android:text="0"
        android:textColor="#ffffff" />

</RelativeLayout>